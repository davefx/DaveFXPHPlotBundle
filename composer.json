{
    "name": "davefx/phplot-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle for PHPlot - Chart toolkit for PHP",
    "keywords": ["davefx", "symfony", "phplot", "charts"],
    "homepage": "https://github.com/davefx/DaveFXPHPlotBundle",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "David Marín Carreño",
            "email": "davefx@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "phplot/phplot",
                "version": "6.1.0",
                "dist": {
                    "url": "http://downloads.sourceforge.net/project/phplot/phplot/6.1.0/phplot-6.1.0.zip",
                    "type": "zip"
                },
                "autoload": {
                    "classmap": ["lib/"]
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": ">=2.0",
        "phplot/phplot": "6.1.0"
    },
    "autoload": {
        "psr-0": { "DaveFX\\Bundle\\PHPlotBundle": "" }
    },
    "target-dir": "DaveFX/Bundle/PHPlotBundle",
    "minimum-stability": "dev"
}
